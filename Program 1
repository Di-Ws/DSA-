import java.util.Scanner;
class Main {
    public static void main(String[] args) {
       
        Scanner sc = new Scanner(System.in);
         System.out.println("Enter the size of element");
         
         int n = sc.nextInt();
         int ar[]=new int[n];
         
         int i,ev=0,od=0;
         for(i=0;i<n;i++){
             System.out.println("Enter the element");
             ar[i]=sc.nextInt();
         }
         for(i=0;i<n;i++){
             if(ar[i]%2==0)
             ev++;
             else
             od++;
         }
         System.out.println("Total count of Even is: " + ev);
         System.out.println("Total count of Odd is: " + od);
    }
}
